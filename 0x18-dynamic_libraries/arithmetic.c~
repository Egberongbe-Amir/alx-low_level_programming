#include <stdio.h>

/**
* add - A function that add two variable
* @a : first variable
* @b : second variable
*
* Return: result
*/
int add(int a, int  b) {
	return a + b;
}

/**
* sub - A function that subtract variable
* @a : first variable
* @b : second variable
*
* Return : result
*/
int sub(int a, int b) {
	return a - b;
}

/**
* mul - A function that multiply variable
* @a : first variable
* @b : second variable
*
* Return : result
*/
int mul(int a, int b) {
	return a * b;                                                                                                            
}
/**
* div - A function that divide variable
* @a : first variable
* @b : second variable
*
* Return : result
*/
int div(int a, int b) {
	return a / b ;
}
/**
* mod - A function that get the modular
* @a : first varibale
* @b : second variable
*
* Return : result
*/
int mod(int a, int b) {
	return a % b
}
